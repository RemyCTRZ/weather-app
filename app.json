{
  "expo": {
    "owner": "sakuto62",
    "name": "MeteoCDA",
    "slug": "meteocda",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/img/icon-OP.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./src/assets/img/icon-OP.png",
      "resizeMode": "cover",
      "backgroundColor": "black"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/img/icon-OP.png",
        "backgroundColor": "black"
      }
    },
    "web": {
      "favicon": "./src/assets/img/icon-OP.png"
    }
  }
}
